{
	"private": false,
	"name": "@synpatico/genome",
	"version": "1.0.2",
	"description": "Deterministic structure ID generation for JavaScript objects using hierarchical hashing and collision-resistant algorithms",
	"author": "Michael Sweeney <overthemike@gmail.com>",
	"type": "module",
	"source": "./src/index.ts",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"build": "pnpm run clean && pnpm run build:types && pnpm run build:js",
		"build:types": "tsc --emitDeclarationOnly --declarationMap",
		"build:js": "tsup",
		"clean": "rm -rf dist",
		"test": "vitest run",
		"prepublishOnly": "pnpm run build",
		"bench": "vitest bench",
		"coverage": "vitest run --coverage",
		"test:ui": "vitest --ui"
	},
	"files": [
		"dist",
		"README.md"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/synpatico/core.git",
		"directory": "packages/genome"
	},
	"bugs": {
		"url": "https://github.com/synpatico/core/issues"
	},
	"homepage": "https://github.com/synpatico/core/tree/main/packages/genome",
	"directories": {
		"test": "tests"
	},
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.3",
		"@types/benchmark": "^2.1.5",
		"@types/node": "^22.13.14",
		"@vitest/coverage-v8": "^3.0.9",
		"@vitest/ui": "^3.0.9",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2",
		"vite": "^6.2.3",
		"vitest": "^3.0.9"
	},
	"keywords": [
		"structure",
		"shape",
		"id",
		"identifier",
		"generator",
		"hash",
		"fingerprint",
		"signature",
		"genome",
		"schema",
		"compare",
		"diff",
		"deduplication",
		"memoization",
		"caching",
		"serialization",
		"object",
		"json",
		"deterministic",
		"unique",
		"synpatico",
		"type"
	]
}
